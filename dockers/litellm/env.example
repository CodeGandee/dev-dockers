# LiteLLM Proxy Security
LITELLM_MASTER_KEY=sk-my-secret-key-123

# External Service Endpoints
# Use 'host.docker.internal' if services are running on the host or other compose stacks
VLLM_API_BASE=http://host.docker.internal:8000/v1
VLLM_API_KEY=EMPTY

LLAMACPP_API_BASE=http://host.docker.internal:8080/v1
LLAMACPP_API_KEY=EMPTY

# Proxy Port Configuration
HOST_PORT_PROXY=4000
CONTAINER_PORT_PROXY=4000
